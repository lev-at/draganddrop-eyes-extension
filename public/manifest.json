{
    "manifest_version": 3,
    "name": "Drag and Drop Applitools Eyes Image Uploader",
    "version": "1.0",
    "permissions": [
      "activeTab","notifications"
    ],
    "host_permissions": [
        "http://localhost/*",
        "https://localhost/*"
    ],
    "action": {"default_popup": "uploader.html"},
    "background": {
        "service_worker": "background.js"
      },
      
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  